@model TesteEstapar.Models.ManobristaVeiculo

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>ManobristaVeiculo</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <p>@Html.DisplayNameFor(model => model.Manobrista)</p>
                <select asp-for="Manobrista" asp-items="@ViewBag.Manobristas"></select>
            </div>
            <div class="form-group">
                <p>@Html.DisplayNameFor(model => model.Veiculo)</p>
                <select asp-for="Veiculo" asp-items="@ViewBag.Veiculos"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
